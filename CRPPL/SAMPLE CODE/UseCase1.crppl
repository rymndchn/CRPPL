varFileName constant 'CRPPL/CSV Files/source/1000 - Movie Reviews.csv'
varDataSetName constant 'CRPPL/CSV Files/target/movies.csv'
do import_file (varFileName,varDataSetName) as movies

legalAge is 18
myAge is 18

if ( myAge GTE legalAge) then
    get rating, sum of budget, sum of gross on movies group_by rating
else
    get rating, sum of budget, sum of gross for rating not_equal 'X' on movies group_by rating
end_if 

do graph(bar, 'rating' is X_AXIS, 'gross' is Y_AXIS, movies)